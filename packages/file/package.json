{
  "name": "@utilz/file",
  "version": "0.3.0",
  "description": "Basic file utilities.",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
