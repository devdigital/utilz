{
  "name": "@utilz/types",
  "version": "0.1.19",
  "description": "Node type utilities",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/devdigital/utilz.git",
    "directory": "packages/types"
  },
  "homepage": "https://github.com/devdigital/utilz/tree/main/packages/types#readme",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "tsup && yarn create-package",
    "watch": "tsup --watch",
    "create-package": "mkdir -p ./output && cp -R ./dist/. ./output/dist && cp package.json ./output"
  },
  "publishConfig": {
    "access": "public",
    "distFolderPath": "."
  },
  "devDependencies": {
    "tsup": "^7.3.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  }
}
