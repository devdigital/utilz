{
  "name": "@utilz/github",
  "version": "0.1.1",
  "description": "GitHub helpers.",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/devdigital/utilz.git",
    "directory": "packages/github"
  },
  "homepage": "https://github.com/devdigital/utilz/tree/main/packages/github#readme",
  "scripts": {
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest --passWithNoTests",
    "build": "tsup && yarn create-package",
