  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@octokit/rest": "^18.12.0",
    "axios": "^0.26.1",
    "cheerio": "^1.0.0-rc.10"
  },
  "devDependencies": {
    "tsup": "^7.3.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  }
}
